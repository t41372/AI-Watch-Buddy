    if (v1 > 0.0 != v2 > 0.0) {
      return 0.0;
    }

    const sign: number = v1 > 0.0 ? 1.0 : -1.0;
    const absoluteValue1 = Math.abs(v1);
    const absoluteValue2 = Math.abs(v2);
    return (
      sign * (absoluteValue1 < absoluteValue2 ? absoluteValue1 : absoluteValue2)
    );
  }

  _startY: number; // x
  _startX: number; // y
  _lastX: number; // x
  _lastY: number; // y
  _lastX1: number; // x
  _lastY1: number; // y
  _lastX2: number; // x
  _lastY2: number; // y
  _lastTouchDistance: number; // 2
  _deltaX: number; // x
  _deltaY: number; // y
  _scale: number; // 1
  _touchSingle: boolean; // true
  _flipAvailable: boolean; // 
}
